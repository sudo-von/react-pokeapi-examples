{
  "name": "dse",
  "main": "dist/bundle.js",
  "types": "src/index.d.ts",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack.production.config.js && npm run build-tailwind",
    "build-storybook": "build-storybook",
    "build-tailwind": "npx tailwindcss -i ./src/index.css -o ./dist/index.css",
    "build-test": "jest --coverage",
    "start": "npm-run-all --parallel start-webpack start-storybook start-tailwind",
    "start-webpack": "webpack --config webpack.development.config.js",
    "start-storybook": "start-storybook -p 3000",
    "start-tailwind": "npm run build-tailwind --watch",
    "start-test": "jest --watch --coverage=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^9.1.2",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
